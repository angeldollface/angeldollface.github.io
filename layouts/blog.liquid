<!DOCTYPE html>
<html>
 {% include 'head' %}
<body>
 {% include 'nav' %}
 {% include 'header' %}
 <div class="columnContainer">
  <div class="contentColumn">
   {% for post in loop_content["posts"] %}
    {% if post.show == 'true' %}
     <div class="listItem">
      <p class="listItem"><a class="listItem" href="{{ post.url }}">{{ post.title }}</a></p>
      <p class="listItemDescription">{{ post.description }}</p>
     </div>
    {% else %}
    {% endif %}
   {% endfor %}
  </div>
 </div>
 {% include 'scripts' %}
</body>
</html>