<!DOCTYPE html>
<html>
 {% include 'head' %}
 <body>
 {% include 'nav' %}
 {% include 'heading' %}
 {% include 'spacer' %}
 <div class="content">
  {% for item in loop_content["posts"] %}
   <div class="postPreview">
   <h2 class="preview">{{ item.title }}</h2>
   <p class="previewDate">{{ item.date  }}</p>
   <p class="previewDesc">{{ item.description }}</p>
   <p class="previewLink">
    <a href="{{ site.baseurl }}{{ item.url }}">{{ site.readMeText }}</a>
   </p>
   </div>
  {% endfor  %}
 </div>
 {% include 'spacer' %}
 {% include 'scripts' %}
 </body>
</html>