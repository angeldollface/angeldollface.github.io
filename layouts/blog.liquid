<html>
 {% include 'head' %}
 <body>
  {% include 'nav' %}
  {% include 'menu' %}
  {% include 'letters' %}
  <div class="content">
   <h2 class="pageHeading">{{ page.title }}</h2>
   {% for post in loop_content["posts"] %}
   <div class="preview">
    <h2 class="preview">{{ post.title }}</h2>
    <p class="preview">{{ post.description }}</p>
    <p class="previewLink">
     <a class="previewLink">{{ site.readMeText }}</a>
    </p>
   </div>
   {% endfor %}
   {% include 'spacer' %}
  </div>
  {% include 'scripts' %}
 </body>
</html>